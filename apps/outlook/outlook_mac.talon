os: mac
and app: Outlook
-
mail:
    key(cmd-1)

calendar:
    key(cmd-2)

contacts:
    key(cmd-3)

open:
    key(cmd-o)

find:
    key(cmd-f)

mail new:
    key(cmd-n)

mail send:
    key(cmd-enter)

mail save:
    key(cmd-s)

mail attach:
    key(cmd-e)

mail reply:
    key(cmd-r)

mail reply all:
    key(shift-cmd-r)

mail forward:
    key(cmd-j)

